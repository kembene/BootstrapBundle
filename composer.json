{
    "name": "kinwae/bootstrap-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 integration with twitter bootstrap",
    "keywords": ["bootstrap","web"],
    "homepage": "http://github.com/kembene",
    "license": "MIT",
    "authors": [
        {
            "name": "Nkem-Etoh Kembene",
            "email": "kembene@kinwae.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1,<2.3-dev",
        "symfony/security-bundle": ">=2.1,<2.3-dev"
    },
    "suggest":  {
        "leafo/lessphp": "dev-master"
    },
    "autoload": {
        "psr-0": { "Kinwae\\BootstrapBundle": "" }
    },
    "target-dir": "Kinwae/BootstrapBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
